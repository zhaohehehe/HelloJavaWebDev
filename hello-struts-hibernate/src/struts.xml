<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
	<package name="hibernate" extends="struts-default" >
		<action name="savePerson" class="com.zhaohe.study.action.PersonAction" method="savePerson">
			<result name="success">/listAll.jsp</result>
		</action>
		<action name="deletePerson" class="com.zhaohe.study.action.PersonAction" method="deletePerson">
			<result name="success">/listAll.jsp</result>
		</action>
		<action name="getSinglePersonById" class="com.zhaohe.study.action.PersonAction" method="getSinglePersonById">
			<result name="success">/getSinglePersonById.jsp</result>
		</action>
		<action name="updatePrePerson" class="com.zhaohe.study.action.PersonAction" method="getSinglePersonById">
			<result name="success">/updatePerson.jsp</result>
		</action>
		<action name="updatePerson" class="com.zhaohe.study.action.PersonAction" method="updatePerson">
			<result name="success">/listAll.jsp</result>
		</action>
	</package>
</struts>